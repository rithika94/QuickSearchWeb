        function SetDateHeaders(startDate, endDate) {
            //debugger;
            var between = [];
            var currentDate = startDate;

            while (currentDate <= endDate) {
                between.push(new Date(currentDate));
                currentDate.setDate(currentDate.getDate() + 1);
            }
            var weekday = new Array(7);
            weekday[0] = "Sunday";
            weekday[1] = "Monday";
            weekday[2] = "Tuesday";
            weekday[3] = "Wednesday";
            weekday[4] = "Thursday";
            weekday[5] = "Friday";
            weekday[6] = "Saturday";

            $("div#Day1").html(weekday[between[0].getDay()] + "</br>" + (between[0].getMonth() + 1) + "/" + between[0].getDate() + "/" + between[0].getFullYear());
            $("div#Day2").html(weekday[between[1].getDay()] + "</br>" + (between[1].getMonth() + 1) + "/" + between[1].getDate() + "/" + between[1].getFullYear());
            $("div#Day3").html(weekday[between[2].getDay()] + "</br>" + (between[2].getMonth() + 1) + "/" + between[2].getDate() + "/" + between[2].getFullYear());
            $("div#Day4").html(weekday[between[3].getDay()] + "</br>" + (between[3].getMonth() + 1) + "/" + between[3].getDate() + "/" + between[3].getFullYear());
            $("div#Day5").html(weekday[between[4].getDay()] + "</br>" + (between[4].getMonth() + 1) + "/" + between[4].getDate() + "/" + between[4].getFullYear());
            $("div#Day6").html(weekday[between[5].getDay()] + "</br>" + (between[5].getMonth() + 1) + "/" + between[5].getDate() + "/" + between[5].getFullYear());
            $("div#Day7").html(weekday[between[6].getDay()] + "</br>" + (between[6].getMonth() + 1) + "/" + between[6].getDate() + "/" + between[6].getFullYear());

        }


