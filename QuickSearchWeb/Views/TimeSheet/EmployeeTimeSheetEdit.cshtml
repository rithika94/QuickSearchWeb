            var Hours3 = $("#TotalHoursDay4").val();
            var Hours4 = $("#TotalHoursDay5").val();
            var Hours5 = $("#TotalHoursDay6").val();
            var Hours6 = $("#TotalHoursDay7").val();
            var totalRegHours = +Hours0 + +Hours1 + +Hours2 + +Hours3 + +Hours4 + +Hours5 + +Hours6;

            var otHrs = 0;

            if (totalRegHours > 40) {
                otHrs = (totalRegHours - 40);
                totalRegHours = 40;
            }



            var totalOTHours = otHrs;
            var totalHours = +totalRegHours + +totalOTHours;


            totalRegHours = Math.round(totalRegHours * 100) / 100;
            totalOTHours = Math.round(totalOTHours * 100) / 100;
            totalHours = Math.round(totalHours * 100) / 100;


            $("#TotalRegularHours").val(totalRegHours);
            $("#TotalOTHours").val(totalOTHours);
            $("#TotalHours").val(totalHours);
        }

